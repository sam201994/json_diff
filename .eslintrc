{ 
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "jest"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "react/jsx-filename-extension": "off",
    "no-restricted-syntax": 0,
    "arrow-parens": 0,
    "import/prefer-default-export": 0,
    "no-use-before-define": "off",
  }
}